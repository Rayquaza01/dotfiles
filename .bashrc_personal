alias cls="clear"
alias nightly="firefox-trunk"
alias t="tmux"

# use 256 color
export TERM=screen-256color
# change path
export PATH="$HOME/.local/bin/:/home/linuxbrew/.linuxbrew/bin/:$PATH"

# open tmux (or attach old session on startup)
[ -z "$TMUX" ] && { tmux attach || tmux new-session; }
# launch neofetch, if exists
# if [ -f /usr/bin/neofetch ]; then neofetch; fi
eval "$(thefuck --alias)"
